{
  "version": 2,
  "builds": [
    {
      "src": "dist/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/health",
      "dest": "/dist/server.js"
    },
    {
      "src": "/api/slack/events",
      "dest": "/dist/server.js"
    },
    {
      "src": "/api/slack/commands",
      "dest": "/dist/server.js"
    },
    {
      "src": "/api/slack/ask-f8",
      "dest": "/dist/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/dist/server.js"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}

